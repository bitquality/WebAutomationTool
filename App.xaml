<Application x:Class="CommandTestAutomation.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Views/MainWindow.xaml"
             
             xmlns:local    ="clr-namespace:CommandTestAutomation.ViewModels">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Assets/WindowDictionary.xaml" />
                <ResourceDictionary Source="Assets/ImageDictionary.xaml" />
                <ResourceDictionary Source="Assets/ButtonDictionary.xaml" />
                <ResourceDictionary Source="Assets/TextBoxDictionary.xaml" />
                <ResourceDictionary Source="Assets/ContextMenuDictionary.xaml" />
                <ResourceDictionary Source="Assets/TreeViewDictionary.xaml" />
                <ResourceDictionary Source="Assets/DataGridResourceDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
