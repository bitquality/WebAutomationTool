<Window x:Class="CommandTestAutomation.Dialogs.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CommandTestAutomation.Dialogs"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Icon="../Images/Application.png"
        Title="About" Height="300" Width="300">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Image  Width="16" Height="16" Source="../Images/Application.png"/>
            <TextBlock Margin="120,0,0,0">BetaTest</TextBlock>
            <TextBlock Margin="110,0,0,0">Version: 1.0</TextBlock>

            <TextBlock Margin="90,50,0,0">Icons: https://icons8.com/</TextBlock>

            <TextBlock Margin="90,90,0,0">Copyright @ 2016-2017</TextBlock>
            <TextBlock Margin="99,10,0,0">All rights reserved.</TextBlock>
            
        </StackPanel>
    </Grid>
</Window>
